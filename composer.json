{
    "name": "noki/large-csv-converter",
    "description": "Fast and memory-efficient Laravel package for converting large CSV, XLSX and JSON files.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Noki\\LargeCsvConverter\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "openspout/openspout": "^5.2",
        "ext-zip": "*",
        "ext-xmlreader": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Noki\\LargeCsvConverter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Noki\\LargeCsvConverter\\LargeCsvConverterServiceProvider"
            ],
            "aliases": {
                "LargeCsvConverter": "Noki\\LargeCsvConverter\\Facades\\LargeCsvConverterFacade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^12.5"
    }
}
